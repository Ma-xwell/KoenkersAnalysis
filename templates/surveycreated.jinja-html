{% extends "layout.jinja-html" %}

{% block title %}
    Survey
{% endblock %}

{% block main %}
    <h1>Your survey has been created.</h1>
    <h3>Here is your survey's ID, please save it.</h3>
    <h2 class="mt-4"><strong>{{ hashed_surveyid }}</strong></h2>
    <h3 class="mt-4">To see the survey, <a href="/insertsurveyid">click here</a> and insert the ID.</h3>
    <h3 class="mt-4">Other people can participate in the survey via the link.</h3>
{% endblock %}